<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
 namespace：必须有值，自定义的唯一字符串
 推荐使用：dao 接口的全限定名称
-->
<mapper namespace="com.huzi.dao.PurchaseOrderDao">
    <!--
    <select>: 查询数据， 标签中必须是 select 语句
    id: sql 语句的自定义名称，推荐使用 dao 接口中方法名称，
    使用名称表示要执行的 sql 语句
    resultType: 查询语句的返回结果数据类型，使用全限定类名
    -->




    <insert id="insertPurchaseOrder" parameterType="com.huzi.domain.purchase.PurchaseOrder">
        insert into t_purchase_order(sku_id,warehouse_id,purchase_amount,purchase_create_time,purchase_state,purchase_update_time)
        values(#{sku_Id},#{warehouse_Id},#{purchase_Amount},#{purchase_Create_Time},#{purchase_State},#{purchase_Update_Time})
    </insert>
</mapper>