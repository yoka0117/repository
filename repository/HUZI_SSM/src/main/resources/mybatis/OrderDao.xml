<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
 namespace：必须有值，自定义的唯一字符串
 推荐使用：dao 接口的全限定名称
-->
<mapper namespace="com.huzi.dao.OrderDao">
    <!--
    <select>: 查询数据， 标签中必须是 select 语句
    id: sql 语句的自定义名称，推荐使用 dao 接口中方法名称，
    使用名称表示要执行的 sql 语句
    resultType: 查询语句的返回结果数据类型，使用全限定类名
    -->


    <select id="" resultType="">
        select id,name,email,age from mybatis
    </select>

    <insert id="insertOrder" parameterType="com.huzi.domain.Order">
        insert into t_order(sku_id,warehouse_id,order_amount,order_create_time,order_state,order_update_time)
        values(#{sku_id},#{warehouse_id},#{order_amount},#{order_create_time},#{order_state},#{order_update_time})
    </insert>
</mapper>